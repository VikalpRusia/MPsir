<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/15.0.1" fx:controller="controller.ShowUsers">
    <TableView fx:id="tableView" prefHeight="400.0" prefWidth="600.0" HBox.hgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="users" prefWidth="599.0" text="User Name"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</HBox>
