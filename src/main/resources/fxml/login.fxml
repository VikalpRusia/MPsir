<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<DialogPane fx:id="dialogPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.DialogController" stylesheets="@../css/login.css">
    <header>
        <Label id="heading" alignment="CENTER" text="Enter details of MySQL"/>
    </header>
    <content>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label text="Username :"/>
            <Label text="Password :" GridPane.rowIndex="1"/>
            <TextField fx:id="username" GridPane.columnIndex="1"/>
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
